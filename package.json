{
  "name": "ext-test-site",
  "version": "0.1.0",
  "private": true,
  "engines": {
    "node": "10.15.1"
  },
  "scripts": {
    "start": "PORT=3010 react-app-rewired start",
    "build": "react-app-rewired build",
    "now-build": "npm run build",
    "deploy": "now ./build --name ext-test-site",
    "test": "react-app-rewired test",
    "test:ci": "CI=true react-app-rewired test",
    "eject": "react-scripts eject",
    "format": "./node_modules/.bin/prettier --write \"src/**/*.{js,jsx,ts,tsx}\"",
    "format:dry": "./node_modules/.bin/prettier -l \"src/**/*.{js,jsx,ts,tsx}\"",
    "types": "./node_modules/.bin/tsc --watch",
    "types:ci": "./node_modules/.bin/tsc"
  },
  "dependencies": {
    "antd": "3.19.3",
    "localforage": "1.7.3",
    "lodash": "4.17.11",
    "react": "16.8.6",
    "react-dom": "16.8.6",
    "react-redux": "7.1.0",
    "redux": "4.0.1",
    "redux-logger": "3.0.6",
    "redux-saga": "1.0.3",
    "reselect": "4.0.0",
    "styled-components": "4.3.1",
    "yup": "0.27.0"
  },
  "devDependencies": {
    "@types/jest": "24.0.15",
    "@types/lodash": "4.14.134",
    "@types/node": "12.0.8",
    "@types/react": "16.8.20",
    "@types/react-dom": "16.8.4",
    "@types/react-redux": "7.1.0",
    "@types/redux-logger": "3.0.7",
    "@types/styled-components": "4.1.8",
    "@types/yup": "0.26.16",
    "babel-plugin-import": "1.12.0",
    "customize-cra": "0.2.13",
    "enzyme": "3.10.0",
    "enzyme-adapter-react-16": "1.14.0",
    "prettier": "1.18.2",
    "react-app-rewired": "2.1.3",
    "react-scripts": "3.0.1",
    "typescript": "3.5.2"
  },
  "prettier": {
    "bracketSpacing": false,
    "printWidth": 100,
    "semi": false,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "jest": {
    "setupFilesAfterEnv": [
      "<rootDir>enzyme-config.js"
    ]
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
