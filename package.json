{
  "name": "ext-test-site",
  "version": "0.1.0",
  "private": true,
  "engines": {
    "node": "10.15.1"
  },
  "scripts": {
    "start": "PORT=3010 react-app-rewired start",
    "build": "react-app-rewired build",
    "now-build": "npm run build",
    "deploy": "now ./build --name ext-test-site",
    "test": "react-app-rewired test",
    "test:ci": "CI=true react-app-rewired test",
    "eject": "react-scripts eject",
    "format": "./node_modules/.bin/prettier --write \"src/**/*.{js,jsx,ts,tsx}\"",
    "format:dry": "./node_modules/.bin/prettier -l \"src/**/*.{js,jsx,ts,tsx}\"",
    "types": "./node_modules/.bin/tsc"
  },
  "dependencies": {
    "antd": "3.15.2",
    "lodash": "4.17.11",
    "react": "16.8.5",
    "react-dom": "16.8.5",
    "react-redux": "6.0.1",
    "redux": "4.0.1",
    "redux-logger": "3.0.6",
    "redux-saga": "1.0.2",
    "reselect": "4.0.0",
    "styled-components": "4.2.0"
  },
  "devDependencies": {
    "@types/jest": "24.0.11",
    "@types/lodash": "4.14.123",
    "@types/node": "11.11.7",
    "@types/react": "16.8.8",
    "@types/react-dom": "16.8.3",
    "@types/react-redux": "7.0.5",
    "@types/redux-logger": "3.0.7",
    "@types/styled-components": "4.1.8",
    "babel-plugin-import": "1.11.0",
    "customize-cra": "0.2.12",
    "enzyme": "3.9.0",
    "enzyme-adapter-react-16": "1.11.2",
    "prettier": "1.16.4",
    "react-app-rewired": "2.1.1",
    "react-scripts": "2.1.8",
    "typescript": "3.3.4000"
  },
  "prettier": {
    "bracketSpacing": false,
    "printWidth": 100,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "jest": {
    "setupTestFrameworkScriptFile": "<rootDir>enzyme-config.js"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
