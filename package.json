{
  "name": "ext-test-site",
  "version": "0.1.0",
  "private": true,
  "engines": {
    "node": "12.x"
  },
  "scripts": {
    "start": "PORT=3010 react-app-rewired start",
    "build": "react-app-rewired build",
    "now-build": "npm run build",
    "deploy": "now ./build --name ext-test-site",
    "test": "react-app-rewired test",
    "test:ci": "CI=true react-app-rewired test",
    "eject": "react-scripts eject",
    "format": "prettier --write \"src/**/*.{js,jsx,ts,tsx}\"",
    "format:dry": "prettier -l \"src/**/*.{js,jsx,ts,tsx}\"",
    "types": "tsc --watch",
    "types:ci": "tsc"
  },
  "dependencies": {
    "antd": "3.23.6",
    "localforage": "1.7.3",
    "lodash": "4.17.15",
    "react": "16.13.0",
    "react-dom": "16.13.0",
    "react-intl": "4.1.1",
    "react-redux": "7.2.0",
    "redux": "4.0.5",
    "redux-logger": "3.0.6",
    "redux-saga": "1.1.3",
    "reselect": "4.0.0",
    "styled-components": "5.0.1",
    "yup": "0.28.3"
  },
  "devDependencies": {
    "@types/jest": "25.1.4",
    "@types/lodash": "4.14.149",
    "@types/node": "13.9.0",
    "@types/react": "16.9.23",
    "@types/react-dom": "16.9.5",
    "@types/react-redux": "7.1.7",
    "@types/redux-logger": "3.0.7",
    "@types/styled-components": "4.1.8",
    "@types/yup": "0.26.32",
    "babel-plugin-import": "1.13.0",
    "customize-cra": "0.9.1",
    "enzyme": "3.11.0",
    "enzyme-adapter-react-16": "1.15.2",
    "prettier": "1.19.1",
    "react-app-rewired": "2.1.5",
    "react-scripts": "3.4.0",
    "typescript": "3.8.3"
  },
  "prettier": {
    "bracketSpacing": false,
    "printWidth": 100,
    "semi": false,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "jest": {
    "setupFilesAfterEnv": [
      "<rootDir>enzyme-config.js"
    ]
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
